{
  "name": "@fox-uni/fob",
  "version": "0.4.3",
  "author": "Taron Foxworth <fox@9generations.com>",
  "main": "./wrapper.js",
  "types": "./wrapper.d.ts",
  "exports": {
    ".": {
      "require": "./wrapper.js",
      "import": "./esm-wrapper.mjs",
      "types": "./wrapper.d.ts"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "napi": {
    "binaryName": "fob-native",
    "packageName": "@fox-uni/fob",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin"
    ]
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "postbuild": "node scripts/sync-binary.js",
    "postbuild:debug": "node scripts/sync-binary.js",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "universal": "napi universalize",
    "version": "napi version",
    "sync-binary": "node scripts/sync-binary.js"
  },
  "optionalDependencies": {
    "@fox-uni/fob-darwin-x64": "0.2.4",
    "@fox-uni/fob-darwin-arm64": "0.2.4"
  }
}
